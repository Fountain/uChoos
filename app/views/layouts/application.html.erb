<!DOCTYPE html>
<html>
<head>
	<title>uChoos</title>
	<%= stylesheet_link_tag :all %>
	<%= javascript_include_tag "http://ajax.googleapis.com/ajax/libs/jquery/1.6.1/jquery.min.js" %>
	<%= javascript_include_tag "http://mediaplayer.yahoo.com/js" %>
	<%= javascript_include_tag :all %>
	<%= csrf_meta_tag %>
	<meta name="viewport" content="width=620">
</head>
<body>
	<div id="page-background"><img src="/images/uchoos_bg_768px_062611.jpg" width="100%" height="100%" alt="starry night"></div>
	<div id="pic-content">
		<div id="container">
		
		<ul class="hmenu">
			<%= render 'devise/menu/registration_items' %> <span style="color: white;">|</span> 
			<%= render 'devise/menu/login_items' %>
		</ul>
		<div id="content">

			<%= yield %>

		</div>
		</div>
	</div>

</body>
</html>
